{% block job_listing_page %}
    <sw-page class="swag-example-list">
        <template slot="content">
            <job-listing-index ref="jobListing"
                               :autoReloadInterval="60000"
                               :jobTypes="klaviyoJobTypes">
            </job-listing-index>
        </template>
        <template #sidebar>
            <sw-sidebar>
                {% block sw_jobs_list_sidebar_refresh %}
                    <sw-sidebar-item
                        icon="default-arrow-360-left"
                        :title="$tc('sw-product.list.titleSidebarItemRefresh')"
                        @click="onRefresh"
                    />
                {% endblock %}
            </sw-sidebar>
        </template>
    </sw-page>
{% endblock %}

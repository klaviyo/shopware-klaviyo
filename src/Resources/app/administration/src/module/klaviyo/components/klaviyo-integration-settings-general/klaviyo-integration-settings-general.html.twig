                                        type="colorpicker"
                                        :map-inheritance="props"
                                        :value="props.currentValue"
                                        :disabled="props.isInherited"
                                        name="KlaviyoIntegrationPlugin.config.popUpOpenBtnColor"
                                        @input="props.updateCurrentValue"></sw-field>
